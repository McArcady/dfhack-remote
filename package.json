{
  "name": "dfhack-remote",
  "version": "1.0.0",
  "description": "DFHack protobuf.js bindings",
  "main": "main.js",
  "scripts": {
    "test": "echo Error: no test specified && exit 1",
    "proto": "protoc --proto_path=proto --js_out=import_style=commonjs,binary:build ./proto/*",
    "protojs": "pbjs proto/*.proto --es6 -t json -o protos.json",
    "build": "browserify main.js -o build/bundle.js"
  },
  "keywords": [
    "dwarf",
    "fortress",
    "dfhack",
    "remote",
    "RemoteFortressReader"
  ],
  "author": "Alex",
  "license": "ISC",
  "dependencies": {
    "browserify": "^16.5.1",
    "google-protobuf": "^3.11.4",
    "protobufjs": "^6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexchandel/dfhack-remote.git"
  }
}
